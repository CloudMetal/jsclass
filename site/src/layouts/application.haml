%html
  %head
    %meta{'http-equiv' => 'content-type', :content => 'text/html; charset=utf-8'}
    %title JS.Class v3.0
    =stylesheets 'application', 'prettify'
    :plain
      <script type="text/javascript" src="http://use.typekit.com/iqy6orq.js"></script>
      <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
      <script type="text/javascript">JSCLASS_PATH = '/javascripts/js.class/';</script>
    =javascripts 'prettify', 'js.class/loader-browser'
  %body{:onload => 'prettyPrint()'}
    #sidebar
      %h1
        =link 'JS.Class', '/'
      #download
        %a{:href => '/assets/JS.Class.3-0-1.zip', :onclick => "trackDownload('3.0.1')"}
